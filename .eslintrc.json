{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
	"env": {
		"browser": true,
		"commonjs": true,
		"es6": true,
		"jquery": true
	},
	"globals": {
		"Discord": true,
		"Materialize": true,
		"Mustache": true,
		"SmoothieChart": true,
		"TimeSeries": true
	},
	"rules": {
		"indent": [2, "tab"],
		"no-tabs": 0,
		"comma-dangle": 0,
		"max-len": 0,
		"no-console": 0,
		"no-param-reassign": 0
	}
}
